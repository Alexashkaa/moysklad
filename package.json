{
  "name": "moysklad",
  "version": "0.3.1",
  "description": "Библиотека для работы с API сервиса МойСклад",
  "main": "index.js",
  "files": [
    "lib",
    "dist",
    "src",
    "tools",
    "index.js",
    "README.md"
  ],
  "scripts": {
    "lint": "eslint src",
    "clean-lib": "rm -rf lib || :",
    "clean-dist": "rm -rf dist || : && mkdir -p dist",
    "transpile": "npm run clean-lib && cp -rf src lib && babel --source-maps --optional runtime --out-dir lib ./src",
    "build": "npm run transpile && npm run clean-dist && browserify --no-builtins --detect-globals false -s MoyskladCore -o dist/moysklad.js lib/index.js",
    "test-src": "node --harmony -r dotenv/config -r isomorphic-fetch `which blue-tape` src/**/*.test.js | tap-spec",
    "test-lib": "blue-tape -r dotenv/config -r isomorphic-fetch lib/**/*.test.js | tap-spec",
    "test": "npm run lint && npm run build && npm run test-lib",
    "coverage": "nyc --silent node --harmony -r dotenv/config -r isomorphic-fetch `which blue-tape` src/**/*.test.js | tap-spec && nyc report --reporter=text --reporter=lcov",
    "coverage-report": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wmakeev/moysklad.git"
  },
  "keywords": [
    "moysklad",
    "warehouse",
    "client",
    "wms",
    "crm",
    "api"
  ],
  "author": {
    "name": "Vitaliy V. Makeev",
    "email": "w.makeev@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wmakeev/moysklad/issues"
  },
  "homepage": "https://github.com/wmakeev/moysklad#readme",
  "dependencies": {
    "@wmakeev/base64encode": "^1.0.0",
    "have2": "1.0.0-beta.1",
    "lodash.defaultsdeep": "^4.6.0",
    "stampit": "^3.1.3"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-es2015-function-name": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-es2015-parameters": "^6.24.1",
    "babel-plugin-transform-es2015-shorthand-properties": "^6.24.1",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "babel-plugin-transform-es2015-sticky-regex": "^6.24.1",
    "babel-plugin-transform-es2015-unicode-regex": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "blue-tape": "^1.0.0",
    "browserify": "^14.4.0",
    "coveralls": "^2.13.1",
    "debug": "^3.0.0",
    "dotenv": "^4.0.0",
    "eslint": "^4.4.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "isomorphic-fetch": "^2.2.1",
    "node-fetch": "^1.7.2",
    "nyc": "^11.1.0",
    "tap-spec": "^4.1.1",
    "walkdir": "0.0.11"
  },
  "engines": {
    "node": ">=4.3.2"
  }
}
